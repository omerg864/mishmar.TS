{
    "version": 2,
    "builds": [
      {
        "src": "./server/src/main.ts",
        "use": "@vercel/node"
      },
      {
        "src": "./client/build",    
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./server/src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        }
    ]
  }